        </activity>
        <activity android:name=".vision.Dashboard"></activity>
        <activity android:name=".vision.ListarVeiculos"></activity>
        <activity android:name=".vision.ListarCliente"></activity>
        <activity android:name=".vision.NovoContrato"></activity>
        <activity android:name=".vision.NovoCliente"></activity>
        <activity android:name=".vision.NovoVeiculo"></activity>
        <activity android:name=".vision.NovoFuncionario"></activity>
        <activity android:name=".vision.ListarContratos"></activity>
        <activity android:name=".vision.ListarFuncionarios"></activity>
        <activity android:name=".vision.Sobre"></activity>
        <activity android:name=".vision.QR_Code" />

    </application>

</manifest>